{
  "kind": "collectionType",
  "collectionName": "companies",
  "info": {
    "name": "Company",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "unique": true
    },
    "categories": {
      "via": "company",
      "collection": "category"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "location": {
      "type": "string"
    },
    "sub_county": {
      "type": "string"
    },
    "brief_description": {
      "type": "richtext",
      "maxLength": 50
    },
    "website_url": {
      "type": "string"
    },
    "official_email": {
      "type": "email"
    },
    "official_tel": {
      "type": "biginteger"
    },
    "logo": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "isPrivate": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "twitter_url": {
      "type": "string"
    },
    "facebook_url": {
      "type": "string"
    },
    "linkedin_url": {
      "type": "string"
    },
    "county": {
      "type": "string"
    }
  }
}
